# Production Test Environment Configuration
# Copy this file to .env and fill in the values for production testing

# Test Mode: 'live' to test against production, 'mock' to skip production tests
PROD_TEST_MODE=mock

# Timeout for production requests (seconds)
PROD_TEST_TIMEOUT=30

# Azure Backend API URL
AZURE-BACKEND-APP-URL=https://up2d8.azurewebsites.net

# Azure Function App URL
AZURE-FUNCTION-APP-URL=https://up2d8-function-app.azurewebsites.net/

# Azure Frontend URL
AZURE-FRONTEND-APP-URL=https://gray-wave-00bdfc60f.3.azurestaticapps.net

# Azure Credentials (for authenticated tests)
AZURE-SUBSCRIPTION-ID=
AZURE-RESOURCE-GROUP-NAME=personal-rg
ENTRA_TENANT_ID=
ENTRA_CLIENT_ID=

# Azure Key Vault (for database access)
KEY_VAULT_URI=https://personal-key-vault1.vault.azure.net/
AZURE-KEY-VAULT-NAME=personal-key-vault1
